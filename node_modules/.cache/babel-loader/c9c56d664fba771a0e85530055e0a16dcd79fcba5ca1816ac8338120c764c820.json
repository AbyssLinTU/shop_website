u0438 \\xAB\\u0441\\u043F\\u0438\\u043D\\u043A\\u0443\\xBB\\n                \\u0431\\u0443\\u0434\\u0435 \\u043F\\u0440\\u043E\\u0441\\u0442\\u043E \\u043D\\u0435\\u0437\\u0430\\u043C\\u0456\\u043D\\u043D\\u043E\\u044E.\"}),/*#__PURE__*/_jsx(\"br\",{}),/*#__PURE__*/_jsx(\"span\",{children:\"\\u041F\\u043E-\\u0434\\u0440\\u0443\\u0433\\u0435, \\u0432\\u0438 \\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u043F\\u0456\\u0434\\u0456\\u0431\\u0440\\u0430\\u0442\\u0438 \\u0440\\u043E\\u0437\\u043C\\u0456\\u0440 \\u0441\\u043F\\u0430\\u043B\\u044C\\u043D\\u043E\\u0433\\u043E \\u043C\\u0456\\u0441\\u0446\\u044F \\u0442\\u0430 \\u0432\\u0438\\u0431\\u0440\\u0430\\u0442\\u0438 \\u0432\\u0431\\u0443\\u0434\\u043E\\u0432\\u0430\\u043D\\u0438\\u0439 \\u043E\\u0440\\u0442\\u043E\\u043F\\u0435\\u0434\\u0438\\u0447\\u043D\\u0438\\u0439 \\u043C\\u0430\\u0442\\u0440\\u0430\\u0446 \\u0434\\u043B\\u044F \\u0431\\u0443\\u0434\\u044C-\\u044F\\u043A\\u043E\\u0457 \\u043C\\u043E\\u0434\\u0435\\u043B\\u0456 \\u043D\\u0430\\u0448\\u0438\\u0445 \\u043B\\u0456\\u0436\\u043E\\u043A (\\u043A\\u0440\\u0456\\u043C \\u043B\\u0456\\u0436\\u043E\\u043A Box Spring). \\u0417\\u0432\\u0435\\u0440\\u043D\\u0456\\u0442\\u044C \\u0443\\u0432\\u0430\\u0433\\u0443, \\u0449\\u043E \\u0432\\u0441\\u0456 \\u043D\\u0430\\u0448\\u0456 \\u043B\\u0456\\u0436\\u043A\\u0430 \\u043F\\u0440\\u043E\\u0434\\u0430\\u044E\\u0442\\u044C\\u0441\\u044F \\u0432\\u0456\\u0434\\u0440\\u0430\\u0437\\u0443 \\u0456\\u0437 \\u0432\\u0431\\u0443\\u0434\\u043E\\u0432\\u0430\\u043D\\u0438\\u043C \\u043C\\u0430\\u0442\\u0440\\u0430\\u0446\\u043E\\u043C (\\u043C\\u0438 \\u043D\\u0435 \\u043F\\u0440\\u043E\\u0434\\u0430\\u0454\\u043C\\u043E \\u043B\\u0456\\u0436\\u043A\\u043E \\u043E\\u043A\\u0440\\u0435\\u043C\\u043E \\u0432\\u0456\\u0434 \\u043C\\u0430\\u0442\\u0440\\u0430\\u0446\\u0430). \\u0441\\u0435\\u0440\\u0442\\u0438\\u0444\\u0456\\u043A\\u0430\\u0442\\u0430\\u043C\\u0438.\"})]})]})]})]}),/*#__PURE__*/_jsx(Offers,{product:state1.product,type:\"new\",title:\"\\u041D\\u043E\\u0432\\u0456 \\u043C\\u043E\\u0434\\u0435\\u043B\\u0456\"}),/*#__PURE__*/_jsx(Offers,{product:state1.product,type:\"popular\",title:\"\\u041F\\u043E\\u043F\\u0443\\u043B\\u044F\\u0440\\u043D\\u0456 \\u043C\\u043E\\u0434\\u0435\\u043B\\u0456\"})]});};","map":{"version":3,"names":["React","useContext","Offers","c","Catalog","Tree","StateContext","jsx","_jsx","jsxs","_jsxs","AboutUs","state1","children","className","container","tree","title","main","catalog","text","block","subtitle","product","type"],"sources":["D:/Git/shop_website/src/components/Pages/AboutUs/AboutUs.jsx"],"sourcesContent":["import React, { useContext } from 'react';\r\nimport { Offers } from '../../utils/Offers/Offers';\r\nimport c from './AboutUs.module.scss';\r\nimport { Catalog } from '../../utils/Catalog/Catalog';\r\nimport { Tree } from '../../utils/Tree/Tree';\r\nimport { StateContext } from '../../../context/context';\r\nexport const AboutUs = () => {\r\n  const { state1 } = useContext(StateContext);\r\n  return (\r\n    <div>\r\n      <div className={c.container}>\r\n        <div className={c.tree}>\r\n          <Tree title=\"О нас\" />\r\n        </div>\r\n        <div className={c.main}>\r\n          <div className={c.catalog}>\r\n            <Catalog />\r\n          </div>\r\n          <div className={c.text}>\r\n            <h2 className={c.title}>Компанія “Divan”</h2>\r\n            <div className={c.block}>\r\n              <h3 className={c.subtitle}>{`М'які меблі для всієї родини`}</h3>\r\n              <span>\r\n                {`Підшукуєте добротні м'які меблі, які прикрашатимуть ваш будинок\r\n                і створюватимуть у ньому атмосферу затишку? Хочете купити зручне\r\n                велике ліжко або гарний диван з кріслами у вітальню? Приходьте\r\n                до нас, фабрика «Divan» – надійний український виробник, який\r\n                випускає красиві та довговічні меблі, якість яких підтверджена\r\n                сертифікатами.`}\r\n              </span>\r\n              <br />\r\n              <span>\r\n                {`Вже понад 20 років фабрика займає почесне місце серед кращих\r\n                українських виробників, пропонуючи покупцям не тільки\r\n                виготовлення м'яких меблів на замовлення, а й готові м'які меблі\r\n                власного виробництва з доставкою по регіонах – Київ та Україна.`}\r\n              </span>\r\n            </div>\r\n            <div className={c.block}>\r\n              <h3 className={c.subtitle}>\r\n                Надійні механізми для частого розкладання\r\n              </h3>\r\n              <span>\r\n                {`Якщо ви вирішили купити диван для повсякденного сну, обов'язково\r\n                зверніть увагу на механізм трансформації. Він повинен бути\r\n                надійним і простим в експлуатації, щоб часте розкладання дивана\r\n                не перетворилося для вас на муки.`}\r\n              </span>\r\n              <br />\r\n              <span>\r\n                {`В основі диванів найпрактичніші механізми трансформації,\r\n                перевірені роками експлуатації та заслужили довіру наших\r\n                клієнтів. Серед них класичні механізми: єврокнижка, Пантограф і\r\n                викочування, а також більш незвичайні, але не менш зручні\r\n                механізми - поворотний, Дельфін і наш унікальний механізм Мааян.\r\n                Кожен механізм відрізняється способом розкладки та підходить для\r\n                різних цілей. Наприклад, для щоденного сну найкраще підійде\r\n                диван із механізмом єврокнижка або Пантограф. В основі\r\n                мініатюрного дивана-малютки, а також для гостьових диванів\r\n                частіше використовується механізм викочування трансформації або\r\n                Дельфін. А для складніших і нестандартніших рішень є поворотний\r\n                механізм і Мааян. Ми пропонуємо різні варіанти, але вибір завжди\r\n                за вами.`}\r\n              </span>\r\n            </div>\r\n            <div className={c.block}>\r\n              <h3 className={c.subtitle}>\r\n                Ліжка з підйомною «спинкою» матраца\r\n              </h3>\r\n              <span>\r\n                {`По-перше, у наших ліжок є суперздатність – підйомна «спинка»\r\n                матраца. Спинка може підніматися за допомогою механічного або\r\n                електричного підйомного механізму.`}\r\n              </span>\r\n              <br />\r\n              <span>\r\n                {`У випадку з механічним витягом вам доведеться вставати з ліжка,\r\n                щоб відрегулювати кут нахилу спинки (всього таких рівнів 11). В\r\n                той час, як електричний підйомний механізм дозволить вам\r\n                залишатися в ліжку та піднімати/опускати спинку натисканням\r\n                кнопки пульта управління. Така опція підйому «спинки» підійде\r\n                всім, хто любить працювати в ліжку з ноутбуком, дивитися серіали\r\n                або читати. А для людей, які проводять багато часу в ліжку за\r\n                станом здоров'я, можливість піднімати та опускати «спинку»\r\n                буде просто незамінною.`}\r\n              </span>\r\n              <br />\r\n              <span>\r\n                По-друге, ви можете підібрати розмір спального місця та вибрати\r\n                вбудований ортопедичний матрац для будь-якої моделі наших ліжок\r\n                (крім ліжок Box Spring). Зверніть увагу, що всі наші ліжка\r\n                продаються відразу із вбудованим матрацом (ми не продаємо ліжко\r\n                окремо від матраца). сертифікатами.\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <Offers product={state1.product} type=\"new\" title=\"Нові моделі\" />\r\n      <Offers\r\n        product={state1.product}\r\n        type=\"popular\"\r\n        title=\"Популярні моделі\"\r\n      />\r\n    </div>\r\n  );\r\n};\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,UAAU,KAAQ,OAAO,CACzC,OAASC,MAAM,KAAQ,2BAA2B,CAClD,MAAO,CAAAC,CAAC,KAAM,uBAAuB,CACrC,OAASC,OAAO,KAAQ,6BAA6B,CACrD,OAASC,IAAI,KAAQ,uBAAuB,CAC5C,OAASC,YAAY,KAAQ,0BAA0B,CAAC,OAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,yBACxD,MAAO,MAAM,CAAAC,OAAO,CAAGA,CAAA,GAAM,CAC3B,KAAM,CAAEC,MAAO,CAAC,CAAGX,UAAU,CAACK,YAAY,CAAC,CAC3C,mBACEI,KAAA,QAAAG,QAAA,eACEH,KAAA,QAAKI,SAAS,CAAEX,CAAC,CAACY,SAAU,CAAAF,QAAA,eAC1BL,IAAA,QAAKM,SAAS,CAAEX,CAAC,CAACa,IAAK,CAAAH,QAAA,cACrBL,IAAA,CAACH,IAAI,EAACY,KAAK,CAAC,2BAAO,CAAE,CAAC,CACnB,CAAC,cACNP,KAAA,QAAKI,SAAS,CAAEX,CAAC,CAACe,IAAK,CAAAL,QAAA,eACrBL,IAAA,QAAKM,SAAS,CAAEX,CAAC,CAACgB,OAAQ,CAAAN,QAAA,cACxBL,IAAA,CAACJ,OAAO,GAAE,CAAC,CACR,CAAC,cACNM,KAAA,QAAKI,SAAS,CAAEX,CAAC,CAACiB,IAAK,CAAAP,QAAA,eACrBL,IAAA,OAAIM,SAAS,CAAEX,CAAC,CAACc,KAAM,CAAAJ,QAAA,CAAC,oEAAgB,CAAI,CAAC,cAC7CH,KAAA,QAAKI,SAAS,CAAEX,CAAC,CAACkB,KAAM,CAAAR,QAAA,eACtBL,IAAA,OAAIM,SAAS,CAAEX,CAAC,CAACmB,QAAS,CAAAT,QAAA,mJAAqC,CAAC,cAChEL,IAAA,SAAAK,QAAA,0vDAOM,CAAC,cACPL,IAAA,QAAK,CAAC,cACNA,IAAA,SAAAK,QAAA,uxCAKM,CAAC,EACJ,CAAC,cACNH,KAAA,QAAKI,SAAS,CAAEX,CAAC,CAACkB,KAAM,CAAAR,QAAA,eACtBL,IAAA,OAAIM,SAAS,CAAEX,CAAC,CAACmB,QAAS,CAAAT,QAAA,CAAC,oOAE3B,CAAI,CAAC,cACLL,IAAA,SAAAK,QAAA,irCAKM,CAAC,cACPL,IAAA,QAAK,CAAC,cACNA,IAAA,SAAAK,QAAA,o/HAcM,CAAC,EACJ,CAAC,cACNH,KAAA,QAAKI,SAAS,CAAEX,CAAC,CAACkB,KAAM,CAAAR,QAAA,eACtBL,IAAA,OAAIM,SAAS,CAAEX,CAAC,CAACmB,QAAS,CAAAT,QAAA,CAAC,4LAE3B,CAAI,CAAC,cACLL,IAAA,SAAAK,QAAA,61BAIM,CAAC,cACPL,IAAA,QAAK,CAAC,cACNA,IAAA,SAAAK,QAAA,0tFAUM,CAAC,cACPL,IAAA,QAAK,CAAC,cACNA,IAAA,SAAAK,QAAA,CAAM,+4CAMN,CAAM,CAAC,EACJ,CAAC,EACH,CAAC,EACH,CAAC,EACH,CAAC,cACNL,IAAA,CAACN,MAAM,EAACqB,OAAO,CAAEX,MAAM,CAACW,OAAQ,CAACC,IAAI,CAAC,KAAK,CAACP,KAAK,CAAC,+DAAa,CAAE,CAAC,cAClET,IAAA,CAACN,MAAM,EACLqB,OAAO,CAAEX,MAAM,CAACW,OAAQ,CACxBC,IAAI,CAAC,SAAS,CACdP,KAAK,CAAC,6FAAkB,CACzB,CAAC,EACC,CAAC,CAEV,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}