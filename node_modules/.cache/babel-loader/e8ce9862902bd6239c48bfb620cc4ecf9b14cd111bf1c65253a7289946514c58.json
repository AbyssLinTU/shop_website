430\\u044E\\u0442\\u044C\\u0441\\u044F \\u0432\\u0456\\u0434\\u0440\\u0430\\u0437\\u0443 \\u0456\\u0437 \\u0432\\u0431\\u0443\\u0434\\u043E\\u0432\\u0430\\u043D\\u0438\\u043C \\u043C\\u0430\\u0442\\u0440\\u0430\\u0446\\u043E\\u043C (\\u043C\\u0438 \\u043D\\u0435 \\u043F\\u0440\\u043E\\u0434\\u0430\\u0454\\u043C\\u043E \\u043B\\u0456\\u0436\\u043A\\u043E \\u043E\\u043A\\u0440\\u0435\\u043C\\u043E \\u0432\\u0456\\u0434 \\u043C\\u0430\\u0442\\u0440\\u0430\\u0446\\u0430). \\u0441\\u0435\\u0440\\u0442\\u0438\\u0444\\u0456\\u043A\\u0430\\u0442\\u0430\\u043C\\u0438.\"\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 84,\n              columnNumber: 15\n            }, this)]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 62,\n            columnNumber: 13\n          }, this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumber: 15,\n          columnNumber: 11\n        }, this)]\n      }, void 0, true, {\n        fileName: _jsxFileName,\n        lineNumber: 13,\n        columnNumber: 9\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 11,\n      columnNumber: 7\n    }, this), /*#__PURE__*/_jsxDEV(Offers, {\n      product: state1.product,\n      type: \"new\",\n      title: \"\\u041D\\u043E\\u0432\\u0456 \\u043C\\u043E\\u0434\\u0435\\u043B\\u0456\"\n    }, void 0, false, {\n      fileName: _jsxFileName,\n      lineNumber: 95,\n      columnNumber: 7\n    }, this), /*#__PURE__*/_jsxDEV(Offers, {\n      product: state1.product,\n      type: \"popular\",\n      title: \"\\u041F\\u043E\\u043F\\u0443\\u043B\\u044F\\u0440\\u043D\\u0456 \\u043C\\u043E\\u0434\\u0435\\u043B\\u0456\"\n    }, void 0, false, {\n      fileName: _jsxFileName,\n      lineNumber: 96,\n      columnNumber: 7\n    }, this)]\n  }, void 0, true, {\n    fileName: _jsxFileName,\n    lineNumber: 10,\n    columnNumber: 5\n  }, this);\n};\n_s(AboutUs, \"QEG8WsPiIOng7D20a1psMTj3eIU=\");\n_c = AboutUs;\nvar _c;\n$RefreshReg$(_c, \"AboutUs\");","map":{"version":3,"names":["React","useContext","Offers","c","Catalog","Tree","StateContext","jsxDEV","_jsxDEV","AboutUs","props","_s","state1","children","className","container","title","fileName","_jsxFileName","lineNumber","columnNumber","main","text","block","subtitle","product","type","_c","$RefreshReg$"],"sources":["D:/Desktop/react-proj/shop/src/components/AboutUs/AboutUs.jsx"],"sourcesContent":["import React, { useContext } from \"react\";\r\nimport { Offers } from \"../Offers/Offers\";\r\nimport c from \"./AboutUs.module.scss\";\r\nimport { Catalog } from \"../Catalog/Catalog\";\r\nimport { Tree } from \"../Tree/Tree\";\r\nimport { StateContext } from \"../../context/context\";\r\nexport const AboutUs = (props) => {\r\n  const { state1 } = useContext(StateContext);\r\n  return (\r\n    <div>\r\n      <div className={c.container}>\r\n        <Tree title=\"О нас\" />\r\n        <div className={c.main}>\r\n          <Catalog />\r\n          <div className={c.text}>\r\n            <h2 className={c.title}>Компанія “Divan”</h2>\r\n            <div className={c.block}>\r\n              <h3 className={c.subtitle}>М'які меблі для всієї родини</h3>\r\n              <span>\r\n                Підшукуєте добротні м'які меблі, які прикрашатимуть ваш будинок\r\n                і створюватимуть у ньому атмосферу затишку? Хочете купити зручне\r\n                велике ліжко або гарний диван з кріслами у вітальню? Приходьте\r\n                до нас, фабрика «Divan» – надійний український виробник, який\r\n                випускає красиві та довговічні меблі, якість яких підтверджена\r\n                сертифікатами.\r\n              </span>\r\n              <br />\r\n              <span>\r\n                Вже понад 20 років фабрика займає почесне місце серед кращих\r\n                українських виробників, пропонуючи покупцям не тільки\r\n                виготовлення м'яких меблів на замовлення, а й готові м'які меблі\r\n                власного виробництва з доставкою по регіонах – Київ та Україна.\r\n              </span>\r\n            </div>\r\n            <div className={c.block}>\r\n              <h3 className={c.subtitle}>\r\n                Надійні механізми для частого розкладання\r\n              </h3>\r\n              <span>\r\n                Якщо ви вирішили купити диван для повсякденного сну, обов'язково\r\n                зверніть увагу на механізм трансформації. Він повинен бути\r\n                надійним і простим в експлуатації, щоб часте розкладання дивана\r\n                не перетворилося для вас на муки.\r\n              </span>\r\n              <br />\r\n              <span>\r\n                В основі диванів найпрактичніші механізми трансформації,\r\n                перевірені роками експлуатації та заслужили довіру наших\r\n                клієнтів. Серед них класичні механізми: єврокнижка, Пантограф і\r\n                викочування, а також більш незвичайні, але не менш зручні\r\n                механізми - поворотний, Дельфін і наш унікальний механізм Мааян.\r\n                Кожен механізм відрізняється способом розкладки та підходить для\r\n                різних цілей. Наприклад, для щоденного сну найкраще підійде\r\n                диван із механізмом єврокнижка або Пантограф. В основі\r\n                мініатюрного дивана-малютки, а також для гостьових диванів\r\n                частіше використовується механізм викочування трансформації або\r\n                Дельфін. А для складніших і нестандартніших рішень є поворотний\r\n                механізм і Мааян. Ми пропонуємо різні варіанти, але вибір завжди\r\n                за вами.\r\n              </span>\r\n            </div>\r\n            <div className={c.block}>\r\n              <h3 className={c.subtitle}>\r\n                Ліжка з підйомною «спинкою» матраца\r\n              </h3>\r\n              <span>\r\n                По-перше, у наших ліжок є суперздатність – підйомна «спинка»\r\n                матраца. Спинка може підніматися за допомогою механічного або\r\n                електричного підйомного механізму.\r\n              </span>\r\n              <br />\r\n              <span>\r\n                У випадку з механічним витягом вам доведеться вставати з ліжка,\r\n                щоб відрегулювати кут нахилу спинки (всього таких рівнів 11). В\r\n                той час, як електричний підйомний механізм дозволить вам\r\n                залишатися в ліжку та піднімати/опускати спинку натисканням\r\n                кнопки пульта управління. Така опція підйому «спинки» підійде\r\n                всім, хто любить працювати в ліжку з ноутбуком, дивитися серіали\r\n                або читати. А для людей, які проводять багато часу в ліжку за\r\n                станом здоров'я, можливість піднімати та опускати «спинку» буде\r\n                просто незамінною.\r\n              </span>\r\n              <br />\r\n              <span>\r\n                По-друге, ви можете підібрати розмір спального місця та вибрати\r\n                вбудований ортопедичний матрац для будь-якої моделі наших ліжок\r\n                (крім ліжок Box Spring). Зверніть увагу, що всі наші ліжка\r\n                продаються відразу із вбудованим матрацом (ми не продаємо ліжко\r\n                окремо від матраца). сертифікатами.\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <Offers product={state1.product} type=\"new\" title=\"Нові моделі\" />\r\n      <Offers\r\n        product={state1.product}\r\n        type=\"popular\"\r\n        title=\"Популярні моделі\"\r\n      />\r\n    </div>\r\n  );\r\n};\r\n"],"mappings":";;AAAA,OAAOA,KAAK,IAAIC,UAAU,QAAQ,OAAO;AACzC,SAASC,MAAM,QAAQ,kBAAkB;AACzC,OAAOC,CAAC,MAAM,uBAAuB;AACrC,SAASC,OAAO,QAAQ,oBAAoB;AAC5C,SAASC,IAAI,QAAQ,cAAc;AACnC,SAASC,YAAY,QAAQ,uBAAuB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AACrD,OAAO,MAAMC,OAAO,GAAIC,KAAK,IAAK;EAAAC,EA